{
  "proxy": {
    "port": 32125,
    "host": "127.0.0.1"
  },

  "gateway": {
    "port": 18789,
    "bind": "lan",
    "auth": {
      "mode": "token",
      "token": "REPLACE_WITH_openssl_rand_hex_32"
    }
  },

  "models": {
    "mode": "merge",
    "providers": {
      "cursor": {
        "baseUrl": "http://127.0.0.1:32125/v1",
        "api": "openai-completions",
        "models": [
          { "id": "auto", "name": "Cursor Auto", "contextWindow": 200000, "maxTokens": 8192 }
        ]
      }
    }
  },

  "agents": {
    "defaults": {
      "model": {
        "primary": "cursor/auto",
        "fallback": "cursor/auto"
      },
      "heartbeat": {
        "enabled": true,
        "interval": "30m",
        "model": "cursor/auto"
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8
      }
    }
  },

  "memory": {
    "enabled": true,
    "embeddings": {
      "model": "text-embedding-3-small"
    },
    "compaction": {
      "mode": "cache-ttl",
      "softThresholdTokens": 40000,
      "cacheTtlHours": 6
    }
  },

  "hooks": {
    "enabled": true,
    "token": "REPLACE_WITH_openssl_rand_hex_32"
  },

  "cron": {
    "enabled": true
  },

  "browser": {
    "enabled": true,
    "defaultProfile": "chrome"
  },

  "logging": {
    "level": "info",
    "pretty": true
  },

  "plugins": [
    "@openclaw/matrix",
    "powermem"
  ],

  "matrix": {
    "homeserver": "https://matrix.org",
    "userId": "@your-bot:matrix.org",
    "accessToken": "YOUR_MATRIX_TOKEN",
    "allowedRooms": [],
    "displayName": "OpenClaw",
    "autoJoin": false
  },

  "compaction": "safeguard"
}
